<% ingredients.slice(start,end).forEach(ingredient => { %>
		<label class="grid_i i_1" for="item_t_<%= ingredient.id %>_<%= gridName %>" data-value="<%= ingredient.id %>" data-color="<%= ingredient.color.hex %>" style="--ingredient_color:<%= ingredient.color.hex %>;" >
			<input type="radio" value="<%= ingredient.id %>" name="<%= gridName %>" id="item_t_<%= ingredient.id %>_<%= gridName %>" >
			<img src="<%- ingredient.asset %>" width="30" alt="item_t_<%= ingredient.name %>_<%= gridName %>">
		</label>
<% }) %>